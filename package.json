{
  "name": "stylesheet-switcher",
  "version": "0.0.0-development",
  "description": "A JavaScript component for switching between a list of stylesheets on a website",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uglow/stylesheet-switcher.git"
  },
  "main": "dist/js/component.es5.js",
  "files": [
    "dist/js/component.es5.*",
    "src",
    "*.md"
  ],
  "scripts": {
    "pre-release": "npm-run-all verify test:coverage test:system:serve build",
    "build": "cross-env NODE_ENV=production npm-run-all clean:prod build:prod build:es5",
    "build:dev": "webpack-dev-server --progress --config config/webpack/dev.webpack.config.js --hot",
    "build:es5": "webpack -p --progress --config config/webpack/prod.es5.webpack.config.js",
    "build:prod": "webpack -p --progress --config config/webpack/prod.webpack.config.js",
    "build:serve": "npm-run-all build serve:prod",
    "clean:dev": "rimraf dev/",
    "clean:prod": "rimraf dist/",
    "precommit": "npm run build",
    "commitmsg": "cz-customizable-ghooks",
    "dev": "cross-env NODE_ENV=development npm-run-all clean:dev --parallel verify:watch build:dev serve:dev",
    "docs:_publish": "node config/docs/publish.js",
    "docs:build": "cross-env NODE_ENV=production webpack --progress --config config/docs/swanky.webpack.config.js --colors",
    "docs:build:serve": "npm-run-all build docs:clean docs:build docs:serve",
    "docs:clean": "rimraf webdocs/",
    "docs:dev": "cross-env NODE_ENV=development node config/docs/serve.dev.js",
    "docs:postpublish": "node config/docs/postpublish.js",
    "docs:prepublish": "node config/docs/prepublish.js",
    "docs:publish": "npm-run-all docs:prepublish docs:clean build docs:build docs:_publish docs:postpublish",
    "docs:serve": "http-server webdocs/ -o",
    "prepush": "npm-run-all verify test:coverage --silent",
    "serve:dev": "echo Starting DEV server",
    "serve:prod": "npm run serve:prod:http",
    "serve:prod:http": "serve -C --compress -p 3000 dist/",
    "serve:prod:https": "serve-https -p 3000 -d dist/ --servername localhost",
    "start": "npm run dev",
    "test": "npm run test:unit",
    "test:system:serve": "npm-run-all --parallel build:dev test:system --race",
    "test:coverage": "npm run test:unit:once",
    "test:system": "cross-env NODE_ENV=test protractor config/testSystem/protractor.conf.js",
    "test:unit": "cross-env BABEL_ENV=test NODE_ENV=test karma start ./config/testUnit/karma.conf.js",
    "test:unit:debug": "cross-env NODE_ENV=test karma start ./config/testUnit/karma.conf.js --debug",
    "test:unit:debug:once": "cross-env NODE_ENV=test karma start --singleRun=true ./config/testUnit/karma.conf.js --debug",
    "test:unit:once": "cross-env BABEL_ENV=test NODE_ENV=test karma start --singleRun=true ./config/testUnit/karma.conf.js",
    "upload-coverage": "cat reports/coverage/lcov/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "verify": "npm-run-all verify:js verify:css --silent",
    "verify:css": "stylint -c config/verify/.stylintrc src/ && echo ✅ verify:css success",
    "verify:css:watch": "chokidar 'src/**/*.styl' -c 'npm run verify:css' --initial --silent",
    "verify:js": "eslint -c config/verify/.eslintrc \"src/**/*.js\" \"src/**/*.jsx\" \"unitTest/**/*.js\" \"unitTest/**/*.jsx\" \"config/**/*.js\" \"config/**/*.jsx\" && echo ✅ verify:js success",
    "verify:js:fix": "eslint --fix -c config/verify/.eslintrc \"src/**/*.js\" \"src/**/*.jsx\" \"unitTest/**/*.js\" \"unitTest/**/*.jsx\" \"config/**/*.js\" \"config/**/*.jsx\" && echo ✅ verify:js:fix success",
    "verify:js:watch": "chokidar 'src/**/*.js' 'src/**/*.jsx' 'unitTest/**/*.js' 'unitTest/**/*.jsx' 'config/**/*.js' 'config/**/*.jsx' -c 'npm run verify:js:fix' --initial --silent",
    "verify:watch": "npm-run-all --parallel verify:js:watch verify:css:watch --silent",
    "semantic-release": "semantic-release && npm run docs:publish",
    "travis-deploy-once": "travis-deploy-once"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "config/release/commitMessageConfig.js"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "2.5.41",
    "@types/selenium-webdriver": "2.44.29",
    "autoprefixer": "6.7.0",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-istanbul": "3.1.2",
    "babel-preset-es2015": "6.22.0",
    "chokidar-cli": "1.2.0",
    "copy-webpack-plugin": "4.0.1",
    "coveralls": "2.11.15",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "cz-customizable": "4.0.0",
    "cz-customizable-ghooks": "1.5.0",
    "eslint": "3.14.1",
    "eslint-config-google": "0.7.1",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "file-loader": "0.10.0",
    "gh-pages": "0.12.0",
    "highlightjs": "9.8.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.28.0",
    "http-server": "0.9.0",
    "husky": "0.13.1",
    "jasmine-core": "2.5.2",
    "jasmine-reporters": "2.2.0",
    "jasmine-spec-reporter": "3.2.0",
    "js-yaml": "3.7.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-es6-shim": "1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-threshold-reporter": "0.1.15",
    "karma-webpack": "2.0.2",
    "latest-version": "2.0.0",
    "npm-run-all": "4.0.1",
    "postcss-loader": "1.2.2",
    "protractor": "3.3.0",
    "rimraf": "2.5.4",
    "semantic-release": "15.6.0",
    "serve": "1.4.0",
    "serve-https": "1.5.3",
    "style-loader": "0.13.1",
    "stylint": "1.5.9",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "svelte": "1.17.2",
    "svelte-loader": "2.0.1",
    "swanky": "2.6.0",
    "swanky-processor-ngdocs": "2.2.3",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0",
    "travis-deploy-once": "5.0.0"
  },
  "engines": {
    "node": ">=6.x",
    "npm": ">=3.9.5"
  }
}
